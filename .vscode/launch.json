  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "app-java",
      "request": "launch",
      "mainClass": "vembry.playground.app.AppApplication",
      "projectName": "app-java"
    },
    {
      "name": "app-go work",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/app-go",
      "args": ["work"]
    },
    {
      "name": "app-go serve",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/app-go",
      "args": ["serve"]
    },
    {
      "name": "app-go dummy",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/app-go",
      "args": ["dumb"]
    },
    {
      "name": "broker",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/broker"
    },
    {
      "name": "load-test - broker",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/load-test",
      "args": ["broker"]
    }
  ]
}
